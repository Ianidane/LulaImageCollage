{
  "name": "Lularoe Collage",
  "version": "1",
  "manifest_version": 2,
  "minimum_chrome_version": "31",
  "web_accessible_resources": [ "fonts/steelfish rg.ttf", "fonts/MavenPro-Regular.ttf" ],
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "permissions": [
    {"fileSystem": ["write", "retainEntries", "directory"]},
    "storage"
  ],
  "file_handlers": {
    "text": {
      "types": [
          "text/*"
      ]
    }
  }
}
